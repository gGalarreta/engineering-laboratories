<div>
  <div class="cold-md-12 form-group row">
    <div class="col-md-3 col-fom-label">
      <%= label_tag :Nombre %>
    </div>
    <div class="col-md-9 input-wrapper">
      <%= f.text_field :client_code, readonly: true , class: "form-control", value: @preliminary_order.name%>
    </div>
  </div>
  <div class="form-group row">
    <div class="col-md-3 col-fom-label">
      <%= label_tag :Codigo_Pucp %>
    </div>
    <div class="col-md-9 input-wrapper">
      <%= f.text_field :pucp_code, class: "form-control", required: "required" %>
    </div>
  </div>
  <div class="form-group row">
    <div class="col-md-3 col-fom-label">
      <%= label_tag :Descripcion %>
    </div>
    <div class="col-md-9 input-wrapper">
      <%= f.text_field :description, class: "form-control", required: "required" %>
    </div>
  </div>
</div>
