<table id="sample-feature" class="table table-hove">
  <thead>
    <tr>
      <th>Nr Muestra</th>
      <% @cols.length.times do |index| %>
        <th><%= @cols[index] %></th>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <% sample.preliminary_order.quantity.to_i.times do |index1| %>
      <tr>
        <td><%= sample.pucp_code + (index1 + 1).to_s %></td>
        <% @cols.length.times do |index2| %>
          <td>
            <div>
              <%= eval(sample.classified_values)[index1.to_s+"-"+index2.to_s] %>
            </div>
          </td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>